{
  "name": "cad3d",
  "version": "0.1.0",
  "private": true,
  "description": "Blank Electron shell backed by a React renderer.",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:renderer\" \"npm:dev:main\"",
    "dev:main": "wait-on tcp:5173 && cross-env NODE_ENV=development VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "dev:renderer": "vite",
    "build": "npm run build:renderer",
    "build:renderer": "vite build",
    "start": "cross-env NODE_ENV=production electron ."
  },
  "keywords": [
    "electron",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^9.114.3",
    "@react-three/fiber": "^8.16.6",
    "electron": "^31.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.160.0",
    "three-csg-ts": "^3.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "vite": "^5.4.2",
    "wait-on": "^7.2.0"
  }
}
